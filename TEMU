local player = game.Players.LocalPlayer
local gui = player:WaitForChild("PlayerGui")
local TweenService = game:GetService("TweenService")

local screenGui = Instance.new("ScreenGui", gui)
screenGui.Name = "MegaTabMenu"
screenGui.ResetOnSpawn = false

local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 420, 0, 500)
mainFrame.Position = UDim2.new(0.5, -210, 0.5, -250)
mainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
mainFrame.BorderSizePixel = 0
mainFrame.BackgroundTransparency = 1
mainFrame.Active = true
mainFrame.Draggable = true

local corner = Instance.new("UICorner", mainFrame)
corner.CornerRadius = UDim.new(0, 12)

TweenService:Create(mainFrame, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {
	BackgroundTransparency = 0.05
}):Play()

-- –ù–∞–¥–ø–∏—Å—å —Å–≤–µ—Ä—Ö—É
local title = Instance.new("TextLabel", mainFrame)
title.Size = UDim2.new(1, 0, 0, 30)
title.Position = UDim2.new(0, 0, 0, -30)
title.Text = "Version Hub"
title.Font = Enum.Font.GothamBold
title.TextSize = 20
title.TextColor3 = Color3.fromRGB(0, 180, 255)
title.BackgroundTransparency = 1

-- –í–∫–ª–∞–¥–∫–∏
local tabsHolder = Instance.new("Frame", mainFrame)
tabsHolder.Size = UDim2.new(1, 0, 0, 45)
tabsHolder.BackgroundTransparency = 1

local tabsLayout = Instance.new("UIListLayout", tabsHolder)
tabsLayout.FillDirection = Enum.FillDirection.Horizontal
tabsLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
tabsLayout.SortOrder = Enum.SortOrder.LayoutOrder
tabsLayout.Padding = UDim.new(0, 10)

-- –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è —Å—Ç—Ä–∞–Ω–∏—Ü
local pagesHolder = Instance.new("Frame", mainFrame)
pagesHolder.Size = UDim2.new(1, 0, 1, -55)
pagesHolder.Position = UDim2.new(0, 0, 0, 50)
pagesHolder.BackgroundTransparency = 1

local pages = {}

local function createTabButton(tabName)
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(0, 120, 0, 35)
	btn.Text = tabName
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 16
	btn.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
	btn.TextColor3 = Color3.fromRGB(200, 230, 255)
	btn.AutoButtonColor = false

	local uic = Instance.new("UICorner", btn)
	uic.CornerRadius = UDim.new(0, 8)

	btn.MouseEnter:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(60, 60, 90)}):Play()
	end)
	btn.MouseLeave:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 60)}):Play()
	end)

	return btn
end

local function createPage(name)
	local page = Instance.new("Frame", pagesHolder)
	page.Name = name
	page.Size = UDim2.new(1, 0, 1, 0)
	page.BackgroundTransparency = 1
	page.Visible = false

	local layout = Instance.new("UIListLayout", page)
	layout.SortOrder = Enum.SortOrder.LayoutOrder
	layout.Padding = UDim.new(0, 12)

	local pad = Instance.new("UIPadding", page)
	pad.PaddingTop = UDim.new(0, 10)
	pad.PaddingLeft = UDim.new(0, 20)
	pad.PaddingRight = UDim.new(0, 20)

	pages[name] = page
	return page
end

local function switchTo(tabName)
	for name, page in pairs(pages) do
		page.Visible = (name == tabName)
	end
end

local function createButton(container, text, callback)
	local btn = Instance.new("TextButton", container)
	btn.Size = UDim2.new(1, 0, 0, 40)
	btn.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
	btn.Text = text
	btn.TextColor3 = Color3.new(1, 1, 1)
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 18
	btn.AutoButtonColor = false

	local uicorner = Instance.new("UICorner", btn)
	uicorner.CornerRadius = UDim.new(0, 10)

	btn.MouseEnter:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {
			BackgroundColor3 = Color3.fromRGB(0, 200, 255)
		}):Play()
	end)
	btn.MouseLeave:Connect(function()
		TweenService:Create(btn, TweenInfo.new(0.2), {
			BackgroundColor3 = Color3.fromRGB(0, 150, 255)
		}):Play()
	end)

	btn.MouseButton1Click:Connect(function()
		if callback then
			callback()
		end
	end)
end

local function createToggle(container, text, callback)
	local holder = Instance.new("Frame", container)
	holder.Size = UDim2.new(1, 0, 0, 40)
	holder.BackgroundTransparency = 1

	local label = Instance.new("TextLabel", holder)
	label.Size = UDim2.new(0.7, 0, 1, 0)
	label.Text = text
	label.TextColor3 = Color3.fromRGB(255, 255, 255)
	label.Font = Enum.Font.Gotham
	label.TextSize = 16
	label.BackgroundTransparency = 1
	label.TextXAlignment = Enum.TextXAlignment.Left

	local toggle = Instance.new("TextButton", holder)
	toggle.Size = UDim2.new(0, 50, 0, 25)
	toggle.Position = UDim2.new(1, -60, 0.5, -12)
	toggle.BackgroundColor3 = Color3.fromRGB(40, 120, 200)
	toggle.Text = ""
	toggle.AutoButtonColor = false

	local toggleCorner = Instance.new("UICorner", toggle)
	toggleCorner.CornerRadius = UDim.new(1, 0)

	local circle = Instance.new("Frame", toggle)
	circle.Size = UDim2.new(0, 20, 0, 20)
	circle.Position = UDim2.new(0, 3, 0.5, -10)
	circle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)

	local circCorner = Instance.new("UICorner", circle)
	circCorner.CornerRadius = UDim.new(1, 0)

	local toggled = false
	toggle.MouseButton1Click:Connect(function()
		toggled = not toggled
		local goal = toggled and UDim2.new(1, -23, 0.5, -10) or UDim2.new(0, 3, 0.5, -10)
		local color = toggled and Color3.fromRGB(0, 255, 120) or Color3.fromRGB(255, 255, 255)
		TweenService:Create(circle, TweenInfo.new(0.25, Enum.EasingStyle.Quad), {
			Position = goal,
			BackgroundColor3 = color
		}):Play()
		if callback then callback(toggled) end
	end)
end

-- üîó –ü—Ä–∏–º–µ—Ä —Ñ—É–Ω–∫—Ü–∏–π
local function RINGV2() local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local root = char:WaitForChild("HumanoidRootPart")

-- üî• –†–µ–∫–ª–∞–º–Ω—ã–µ —Ñ—Ä–∞–∑—ã TEMU
local phrases = {
	"üî• 90% OFF üî•",
	"BLACK FRIDAY EVERYDAY",
	"BUY 1 GET 7 FREE",
	"FREE GIFT INSIDE üéÅ",
	"TEMU PREMIUM USER",
	"üò± LIMITED TIME OFFER",
	"üõçÔ∏è ADD TO CART NOW",
	"üí∞ SAVE BIG ON TEMU"
}

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∏
local ringCount = #phrases -- —Å—Ç–æ–ª—å–∫–æ, —Å–∫–æ–ª—å–∫–æ —Ñ—Ä–∞–∑
local radius = 4
local rotationSpeed = 1
local tiltAngle = math.rad(30)

-- –ü–∞–ø–∫–∞ –¥–ª—è –∫–æ–ª—å—Ü–∞
local temuRing = Instance.new("Folder")
temuRing.Name = "TEMUAdRing"
temuRing.Parent = root

-- –°–æ–∑–¥–∞–Ω–∏–µ –æ–¥–Ω–æ–≥–æ –∫—Ä—É—Ç—è—â–µ–≥–æ—Å—è —Ä–µ–∫–ª–∞–º–Ω–æ–≥–æ –±–ª–æ–∫–∞
local function createRingLabel(text)
	local part = Instance.new("Part")
	part.Anchored = true
	part.CanCollide = false
	part.Transparency = 1
	part.Size = Vector3.new(4, 1, 0.1)
	part.Parent = temuRing

	local gui = Instance.new("BillboardGui")
	gui.Size = UDim2.new(4, 0, 1, 0)
	gui.Adornee = part
	gui.AlwaysOnTop = true
	gui.LightInfluence = 0.5
	gui.MaxDistance = 100
	gui.Parent = part

	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 1, 0)
	label.BackgroundTransparency = 1
	label.Text = text
	label.TextColor3 = Color3.fromRGB(255, 128, 0)
	label.Font = Enum.Font.FredokaOne
	label.TextScaled = true
	label.Parent = gui

	return part
end

-- –°–æ–±–∏—Ä–∞–µ–º –∫–æ–ª—å—Ü–æ –∏–∑ —Ñ—Ä–∞–∑
local labels = {}
for i, text in ipairs(phrases) do
	local part = createRingLabel(text)
	table.insert(labels, part)
end

-- –ö—Ä—É—Ç–∏–º –∫–æ–ª—å—Ü–æ
RunService.RenderStepped:Connect(function()
	local time = tick() * rotationSpeed
	for i, part in ipairs(labels) do
		local angle = (i / #labels) * math.pi * 2 + time
		local x = math.cos(angle) * radius
		local z = math.sin(angle) * radius
		local y = math.sin(tiltAngle) * z

		part.CFrame = root.CFrame:ToWorldSpace(CFrame.new(x, y, z))
	end
end) end



local function SKIN() local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()

-- –£–¥–∞–ª—è–µ–º —à–º–æ—Ç–∫–∏ –∏ –∞–∫—Å–µ—Å—Å—É–∞—Ä—ã
for _, item in ipairs(char:GetChildren()) do
	if item:IsA("Accessory") or item:IsA("Shirt") or item:IsA("Pants") or item:IsA("ShirtGraphic") then
		item:Destroy()
	end
end

-- –ù–∞–¥–ø–∏—Å—å TEMU –Ω–∞ –æ–¥–Ω–æ–π —Å—Ç–æ—Ä–æ–Ω–µ
local function addTemuLabel(part)
	-- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –Ω–µ –¥–æ–±–∞–≤–ª—è–ª–∏ –ª–∏ –º—ã —É–∂–µ –Ω–∞–¥–ø–∏—Å—å
	if part:FindFirstChildWhichIsA("SurfaceGui") then return end

	local gui = Instance.new("SurfaceGui")
	gui.Name = "TEMU_Label"
	gui.Face = Enum.NormalId.Front
	gui.AlwaysOnTop = false -- –Ω–µ –≤–∏–¥–Ω–æ —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã
	gui.SizingMode = Enum.SurfaceGuiSizingMode.PixelsPerStud
	gui.PixelsPerStud = 50
	gui.Parent = part

	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 1, 0)
	label.BackgroundTransparency = 1
	label.Text = "TEMU"
	label.TextScaled = true
	label.TextColor3 = Color3.new(1, 1, 1)
	label.Font = Enum.Font.FredokaOne
	label.Parent = gui
end

-- –ö—Ä–∞—Å–∏–º –∏ –≤–µ—à–∞–µ–º –Ω–∞–¥–ø–∏—Å—å —Ç–æ–ª—å–∫–æ –Ω–∞ —á–∞—Å—Ç–∏ —Ç–µ–ª–∞
for _, part in ipairs(char:GetChildren()) do
	if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
		part.BrickColor = BrickColor.new("Bright orange")
		addTemuLabel(part)
	end
end
 end


local function BOX() local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local backpack = lp:WaitForChild("Backpack")

-- —Å–æ–∑–¥–∞—ë–º –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç (Tool)
local tool = Instance.new("Tool")
tool.Name = "HeavyBox"
tool.RequiresHandle = true
tool.CanBeDropped = true

-- —Å–æ–∑–¥–∞—ë–º –∫–æ—Ä–æ–±–∫—É
local handle = Instance.new("Part")
handle.Name = "Handle"
handle.Size = Vector3.new(2.5, 2, 2)
handle.BrickColor = BrickColor.new("Bright orange")
handle.Material = Enum.Material.SmoothPlastic
handle.Anchored = false
handle.CanCollide = false
handle.TopSurface = Enum.SurfaceType.Smooth
handle.BottomSurface = Enum.SurfaceType.Smooth
handle.Parent = tool

-- –Ω–æ–≤–æ–µ —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏–µ ‚Äî –ø—Ä—è–º –º–µ–∂–¥—É —Ä—É–∫–∞–º–∏
tool.Grip = CFrame.new(1.5, 1, -0) * CFrame.Angles(math.rad(0), 0, 0)

tool.Parent = backpack end


local function TUSER() local Players = game:GetService("Players")
local lp = Players.LocalPlayer  -- –õ–æ–∫–∞–ª—å–Ω—ã–π –∏–≥—Ä–æ–∫

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –Ω–∞–¥–ø–∏—Å–∏ –Ω–∞–¥ –∏–≥—Ä–æ–∫–æ–º
local function createLabel(player, text)
    local character = player.Character
    if not character then return end

    -- –°–æ–∑–¥–∞–µ–º BillboardGui, –∫–æ—Ç–æ—Ä—ã–π –±—É–¥–µ—Ç –Ω–∞–¥ –≥–æ–ª–æ–≤–æ–π –∏–≥—Ä–æ–∫–∞
    local billBoard = Instance.new("BillboardGui")
    billBoard.Adornee = character:WaitForChild("Head") -- —Ä–∞—Å–ø–æ–ª–∞–≥–∞—Ç—å –Ω–∞–¥ –≥–æ–ª–æ–≤–æ–π
    billBoard.Size = UDim2.new(0, 200, 0, 50)  -- —Ä–∞–∑–º–µ—Ä –Ω–∞–¥–ø–∏—Å–∏
    billBoard.StudsOffset = Vector3.new(0, 3, 0)  -- –æ—Ç—Å—Ç—É–ø –æ—Ç –≥–æ–ª–æ–≤—ã
    billBoard.Parent = character:WaitForChild("Head")

    -- –°–æ–∑–¥–∞–µ–º —Ç–µ–∫—Å—Ç–æ–≤—É—é –º–µ—Ç–∫—É
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, 0, 1, 0)  -- —Ä–∞–∑–º–µ—Ä –Ω–∞–¥–ø–∏—Å–∏
    label.BackgroundTransparency = 1
    label.Text = text
    label.TextScaled = true
    label.TextColor3 = Color3.fromRGB(255, 223, 0)  -- –∑–æ–ª–æ—Ç–æ–π —Ü–≤–µ—Ç
    label.Font = Enum.Font.GothamBold
    label.Parent = billBoard
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –Ω–∞–¥–ø–∏—Å–∏ –¥–ª—è –ª–æ–∫–∞–ª—å–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞
createLabel(lp, "TEMU Premium User")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –≤—ã–±–æ—Ä–∞ —Å–ª—É—á–∞–π–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –Ω–∞ —Å–µ—Ä–≤–µ—Ä–µ (–∫—Ä–æ–º–µ –ª–æ–∫–∞–ª—å–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞)
local function getRandomPlayer()
    local players = Players:GetPlayers()
    local randomPlayer = nil

    -- –ò—â–µ–º —Å–ª—É—á–∞–π–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞, –∫–æ—Ç–æ—Ä—ã–π –Ω–µ —è–≤–ª—è–µ—Ç—Å—è –ª–æ–∫–∞–ª—å–Ω—ã–º
    repeat
        randomPlayer = players[math.random(1, #players)]
    until randomPlayer ~= lp  -- –ø—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ–±—ã —ç—Ç–æ –Ω–µ –±—ã–ª –ª–æ–∫–∞–ª—å–Ω—ã–π –∏–≥—Ä–æ–∫

    return randomPlayer
end

-- –í—ã–±–∏—Ä–∞–µ–º —Å–ª—É—á–∞–π–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –∏ –¥–æ–±–∞–≤–ª—è–µ–º –Ω–∞–¥–ø–∏—Å—å –ø—Ä–æ —Ö–∞–±
local randomPlayer = getRandomPlayer()
createLabel(randomPlayer, "Version Hub the best")
 end


local function Backpack() local Players = game:GetService("Players")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local backpack = lp:WaitForChild("Backpack")

-- —Å–æ–∑–¥–∞—ë–º –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç (Tool)
local tool = Instance.new("Tool")
tool.Name = "TemuPackage"
tool.RequiresHandle = true
tool.CanBeDropped = true

-- —Å–æ–∑–¥–∞—ë–º –ø–ª–æ—Å–∫–∏–π –ø–∞–∫–µ—Ç
local handle = Instance.new("Part")
handle.Name = "Handle"
handle.Size = Vector3.new(2, 2, 1)
handle.BrickColor = BrickColor.new("Bright orange")
handle.Material = Enum.Material.SmoothPlastic
handle.Anchored = false
handle.CanCollide = false
handle.TopSurface = Enum.SurfaceType.Smooth
handle.BottomSurface = Enum.SurfaceType.Smooth
handle.Parent = tool

tool.Grip = CFrame.new(1.4, 0.5, -2.7) -- —Ä–µ–¥–∞–∫—Ç–∏—Ä—É–π –ø–æ–¥ —Å–µ–±—è

-- —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è –Ω–∞–¥–ø–∏—Å–µ–π
local function addTemuLabel(surface)
	local gui = Instance.new("SurfaceGui")
	gui.Face = surface
	gui.AlwaysOnTop = true
	gui.SizingMode = Enum.SurfaceGuiSizingMode.PixelsPerStud
	gui.PixelsPerStud = 50
	gui.Parent = handle

	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 1, 0)
	label.BackgroundTransparency = 1
	label.Text = "TEMU"
	label.TextScaled = true
	label.TextColor3 = Color3.new(1, 1, 1)
	label.Font = Enum.Font.FredokaOne
	label.Parent = gui
end

-- TEMU –Ω–∞–¥–ø–∏—Å–∏ –Ω–∞ –≤—Å–µ—Ö —Å—Ç–æ—Ä–æ–Ω–∞—Ö
local surfaces = {
	Enum.NormalId.Back,
}

for _, surface in ipairs(surfaces) do
	addTemuLabel(surface)
end

-- üßµ —Å–æ–∑–¥–∞—ë–º 2 "–≤–µ—Ä—ë–≤–æ—á–∫–∏" —Å —É–¥–ª–∏–Ω—ë–Ω–Ω–æ–π –¥—É–≥–æ–π
local function createString(offsetX)
	-- –°–æ–∑–¥–∞–µ–º –Ω–µ—Å–∫–æ–ª—å–∫–æ —á–∞—Å—Ç–µ–π –¥–ª—è –≤–µ—Ä—ë–≤–æ—á–∫–∏ (–¥—É–≥–∞), –Ω–æ —É–¥–ª–∏–Ω—è–µ–º
	for i = 0, 10 do  -- —É–≤–µ–ª–∏—á–∏–ª –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ —á–∞—Å—Ç–µ–π –¥–ª—è –ø–ª–∞–≤–Ω–æ—Å—Ç–∏ –¥—É–≥–∏
		local stringPart = Instance.new("Part")
		stringPart.Size = Vector3.new(0.1, 0.1, 0.2)  -- —É–≤–µ–ª–∏—á–∏–ª –¥–ª–∏–Ω—É –∫–∞–∂–¥–æ–π —á–∞—Å—Ç–∏
		stringPart.BrickColor = BrickColor.Black()
		stringPart.Material = Enum.Material.SmoothPlastic
		stringPart.CanCollide = false
		stringPart.Anchored = false
		stringPart.Massless = true
		stringPart.Name = "String"

		-- –°–æ–∑–¥–∞–µ–º WeldConstraint –¥–ª—è –∫—Ä–µ–ø–ª–µ–Ω–∏—è
		local weld = Instance.new("WeldConstraint")
		weld.Part0 = handle
		weld.Part1 = stringPart
		weld.Parent = stringPart

		-- –ü—Ä–∏–º–µ–Ω—è–µ–º CFrame –¥–ª—è –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ–≥–æ —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–∏—è –∫–∞–∂–¥–æ–π —á–∞—Å—Ç–∏
		stringPart.CFrame = handle.CFrame * CFrame.new(offsetX, 0.35 + (i * 0.05), -0.9 + (i * 0.05))  -- —Ä–µ–≥—É–ª–∏—Ä—É–µ–º –æ—Ç—Å—Ç—É–ø
		stringPart.Parent = handle
	end
end

-- –¥–≤–µ —Ä—É—á–∫–∏: –ª–µ–≤–∞—è –∏ –ø—Ä–∞–≤–∞—è
createString(-0.9)
createString(0.9)

-- –¥–æ–±–∞–≤–ª—è–µ–º –≤ –∏–Ω–≤–µ–Ω—Ç–∞—Ä—å
tool.Parent = backpack
 end


local function RINGV1() local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

-- –°–ø–∏—Å–æ–∫ –Ω–∞–¥–ø–∏—Å–µ–π (–∞—É—Ä—ã)
local auraParts = {}

-- –°–æ–∑–¥–∞—ë–º –æ–¥–Ω—É –Ω–∞–¥–ø–∏—Å—å –∏ –≤–æ–∑–≤—Ä–∞—â–∞–µ–º –µ—ë
local function createAuraLabel(angleOffset)
	local part = Instance.new("Part")
	part.Size = Vector3.new(1, 1, 1)
	part.Anchored = true
	part.CanCollide = false
	part.Transparency = 1
	part.Name = "TEMUAura"
	part.Parent = workspace

	local gui = Instance.new("BillboardGui")
	gui.Size = UDim2.new(4, 0, 1, 0)
	gui.AlwaysOnTop = true
	gui.LightInfluence = 0
	gui.MaxDistance = 100
	gui.Parent = part

	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 1, 0)
	label.BackgroundTransparency = 1
	label.Text = "TEMU"
	label.TextScaled = true
	label.TextColor3 = Color3.fromRGB(255, 130, 0)
	label.Font = Enum.Font.FredokaOne
	label.Parent = gui

	table.insert(auraParts, { part = part, angle = angleOffset })
end

-- –°–æ–∑–¥–∞—ë–º –∫—Ä—É–≥ –∏–∑ –Ω–∞–¥–ø–∏—Å–µ–π –≤–æ–∫—Ä—É–≥ –∏–≥—Ä–æ–∫–∞
for i = 1, 8 do
	createAuraLabel((i / 8) * math.pi * 2)
end

-- –û–±–Ω–æ–≤–ª—è–µ–º –ø–æ–ª–æ–∂–µ–Ω–∏–µ –∞—É—Ä—ã
RunService.RenderStepped:Connect(function(dt)
	local time = tick()
	for _, aura in ipairs(auraParts) do
		local radius = 5
		local height = 2
		local angle = aura.angle + time * 1 -- –º–æ–∂–Ω–æ –º–µ–Ω—è—Ç—å —Å–∫–æ—Ä–æ—Å—Ç—å

		local x = math.cos(angle) * radius
		local z = math.sin(angle) * radius
		local y = math.sin(angle * 2) * 0.5 + height

		aura.part.Position = hrp.Position + Vector3.new(x, y, z)
	end
end)
 end


local function Jump() local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local humanoid = char:WaitForChild("Humanoid")
local root = char:WaitForChild("HumanoidRootPart")

local debounce = false

local function createBuyNowText()
	local part = Instance.new("Part")
	part.Anchored = true
	part.CanCollide = false
	part.Size = Vector3.new(3, 0.5, 1)
	part.CFrame = root.CFrame * CFrame.new(0, -3, 0)
	part.Transparency = 1
	part.Parent = workspace

	local gui = Instance.new("BillboardGui")
	gui.Size = UDim2.new(5, 0, 2, 0)
	gui.Adornee = part
	gui.AlwaysOnTop = true
	gui.LightInfluence = 0
	gui.Parent = part

	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 1, 0)
	label.BackgroundTransparency = 1
	label.Text = "BUY NOW!"
	label.TextColor3 = Color3.fromRGB(255, 85, 0)
	label.TextStrokeTransparency = 0.2
	label.Font = Enum.Font.FredokaOne
	label.TextScaled = true
	label.Parent = gui

	game.Debris:AddItem(part, 0.8)
end

humanoid.StateChanged:Connect(function(old, new)
	if new == Enum.HumanoidStateType.Jumping and not debounce then
		debounce = true
		createBuyNowText()
		wait(0.5)
		debounce = false
	end
end)
 end


local function Trail() local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Debris = game:GetService("Debris")
local lp = Players.LocalPlayer
local char = lp.Character or lp.CharacterAdded:Wait()
local root = char:WaitForChild("HumanoidRootPart")

-- üí® –£–¥–∞–ª—è–µ–º —Å—Ç–∞—Ä—ã–µ —Ç—Ä–µ–π–ª—ã
for _, v in ipairs(root:GetChildren()) do
	if v:IsA("Trail") then
		v:Destroy()
	end
end

-- üìç–°–æ–∑–¥–∞—ë–º Attachment'—ã –¥–ª—è —Ç—Ä–µ–π–ª–∞
local att0 = Instance.new("Attachment", root)
att0.Position = Vector3.new(0, 1.5, 0)

local att1 = Instance.new("Attachment", root)
att1.Position = Vector3.new(0, -1.5, 0)

-- üß° –°–æ–∑–¥–∞—ë–º —Å–∞–º Trail
local trail = Instance.new("Trail")
trail.Attachment0 = att0
trail.Attachment1 = att1
trail.Color = ColorSequence.new(Color3.fromRGB(255, 128, 0))
trail.Lifetime = 0.5
trail.MinLength = 0.1
trail.LightEmission = 1
trail.Name = "TEMUTrail"
trail.Transparency = NumberSequence.new({
	NumberSequenceKeypoint.new(0, 0),
	NumberSequenceKeypoint.new(1, 1)
})
trail.WidthScale = NumberSequence.new(0.6)
trail.Parent = root

-- ‚ú® –¢–µ–º–Ω—ã–π –ø–∞—Ä—è—â–∏–π —Å–ª–µ–¥ –∏–∑ —Ç–µ–∫—Å—Ç–∞ TEMU
local interval = 0.5
local textLifetime = 0.5
local offset = Vector3.new(0, 0, -1)
local lastTime = 0

local function createTemuLabel()
	local part = Instance.new("Part")
	part.Anchored = true
	part.CanCollide = false
	part.Size = Vector3.new(2, 0.5, 0.1)
	part.CFrame = root.CFrame:ToWorldSpace(CFrame.new(offset))
	part.Transparency = 1
	part.Parent = workspace

	local gui = Instance.new("BillboardGui")
	gui.Size = UDim2.new(3, 0, 1, 0)
	gui.Adornee = part
	gui.AlwaysOnTop = true
	gui.LightInfluence = 0
	gui.Parent = part

	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(1, 0, 1, 0)
	label.BackgroundTransparency = 1
	label.Text = "TEMU"
	label.TextColor3 = Color3.fromRGB(255, 128, 0)
	label.Font = Enum.Font.FredokaOne
	label.TextScaled = true
	label.Parent = gui

	Debris:AddItem(part, textLifetime)
end

-- üîÅ –ü–æ—Å—Ç–æ—è–Ω–Ω—ã–π —Ä–µ–Ω–¥–µ—Ä –¥–ª—è –Ω–∞–¥–ø–∏—Å–µ–π
RunService.RenderStepped:Connect(function()
	local hum = char:FindFirstChildOfClass("Humanoid")
	if hum and hum.MoveDirection.Magnitude > 0 then
		local now = tick()
		if now - lastTime >= interval then
			createTemuLabel()
			lastTime = now
		end
	end
end)
 end


-- üìÅ –ö–æ–Ω—Ñ–∏–≥
local tabs = {
	{
		Name = "TEMU Scripts",
		Buttons = {
			["TEMU Ring V2"] = RINGV2,
			["TEMU Skin"] = SKIN,
            ["TEMU Box"] = BOX,
			["TEMU User"] = TUSER,
            ["TEMU Backpack"] = Backpack,
            ["TEMU Ring V1"] = RINGV1,
            ["TEMU Jump Effect"] = Jump,
            ["TEMU Trail"] = Trail
		},
		Toggles = {
		}
	},
}

-- üß† –°–æ–∑–¥–∞–Ω–∏–µ –≤–∫–ª–∞–¥–æ–∫
for _, tabData in pairs(tabs) do
	local tabBtn = createTabButton(tabData.Name)
	tabBtn.Parent = tabsHolder

	local page = createPage(tabData.Name)

	for btnText, func in pairs(tabData.Buttons or {}) do
		createButton(page, btnText, func)
	end

	for toggleText, func in pairs(tabData.Toggles or {}) do
		createToggle(page, toggleText, func)
	end

	tabBtn.MouseButton1Click:Connect(function()
		switchTo(tabData.Name)
	end)
end

switchTo(tabs[1].Name)
